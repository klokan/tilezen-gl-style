      }
    },
    {
      "id": "water-line",
      "type": "line",
      "source": "tilezen",
      "source-layer": "water",
      "filter": [
        "==",
        "$type",
        "LineString"
      ],
      "paint": {
        "line-color": "#7acad0",
        "line-width": {
          "base": 1.2,
          "stops": [
            [
              8,
              0.5
            ],
            [
              20,
              15
            ]
          ]
        }
      }
    },
    {
      "id": "water-polygon",
      "type": "fill",
      "source": "tilezen",
